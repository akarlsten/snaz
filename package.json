{
  "name": "snaz",
  "displayName": "Snaz",
  "version": "2.0.1",
  "homepage": "https://github.com/akarlsten/snaz",
  "description": "A dark and pleasant color theme.",
  "publisher": "akarlsten",
  "engines": {
    "vscode": "^1.22.0"
  },
  "bugs": { "url": "https://github.com/akarlsten/snaz/issues"},
  "scripts": {
    "build": "node ./scripts/build.js",
    "lint": "node ./scripts/lint.js",
    "package": "vsce package -o ./bin/snaz.vsix",
    "vscode:prepublish": "npm run build",
    "vsce-publish": "vsce publish",
    "dev:watch": "watch 'npm run build' ./src"
  },
  "author": "Adam Karlsten <adamkarlsten@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akarlsten/snaz.git"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "prettier": "^2.4.1",
    "tinycolor2": "^1.4.2",
    "vsce": "^1.100.1"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "snaz",
    "dark",
    "theme",
    "color-theme",
    "icon-theme"
  ],
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#24283e",
    "theme": "dark"
  },
  "contributes": {
    "themes": [
      {
        "label": "Snaz",
        "uiTheme": "vs-dark",
        "path": "./theme/snaz.json"
      }
    ],
    "iconThemes": [
      {
        "id": "snaz-icons",
        "label": "Snaz Icons",
        "path": "./icons.json"
      }
    ]
  },
  "devDependencies": {
    "watch": "^1.0.2"
  }
}
